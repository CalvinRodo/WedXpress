table.table.table-hover.table-condensed
  thead
    tr
      th Name
      th Course
      th Actions
  tbody
    - each menuItem in menuItems
      tr
      th #{menuItem.name}
      th #{menuItem.course}
      th
        .btn-group
          a(href='/menuItem/delete/#{menuItem._id}').btn
            i.icon-remove
